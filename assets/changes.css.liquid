  src: url({{'bebasneue_regular-webfont.eot?#iefix' | asset_url }}) format('embedded-opentype'),
    url({{'bebasneue_regular-webfont.woff2' | asset_url }}) format('woff2'),
    url({{'bebasneue_regular-webfont.woff' | asset_url }}) format('woff'),
    url({{'bebasneue_regular-webfont.ttf' | asset_url }}) format('truetype'),
    url({{'bebasneue_regular-webfont.svg#bebas_neue_regularregular' | asset_url }}) format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: 'bebas_neuebook';
  src: url({{'bebasneue_book-webfont.eot' | asset_url }});
  src: url({{'bebasneue_book-webfont.eot?#iefix' | asset_url }}) format('embedded-opentype'),
    url({{'bebasneue_book-webfont.woff2' | asset_url }}) format('woff2'),
    url({{'bebasneue_book-webfont.woff' | asset_url }}) format('woff'),
    url({{'bebasneue_book-webfont.ttf' | asset_url }}) format('truetype'),
    url({{'bebasneue_book-webfont.svg#bebas_neuebook' | asset_url }}) format('svg');
  font-weight: normal;
  font-style: normal;

}
@font-face {
  font-family: 'bebas_neuebold';
  src: url({{'bebasneue_bold-webfont.eot' | asset_url }});
  src: url({{'bebasneue_bold-webfont.eot?#iefix' | asset_url }}) format('embedded-opentype'),
    url({{'bebasneue_bold-webfont.woff2' | asset_url }}) format('woff2'),
    url({{'bebasneue_bold-webfont.woff' | asset_url }}) format('woff'),

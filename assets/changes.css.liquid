#pageheader {
  width:152px;
  transition:1s all;
}
.logo a:link, .logo a:visited, .logo {
  height:56px;
  width: 152px;
  background:none !important;
}
.logo .logo_text {
  padding-top:16px;
  letter-spacing: 0.01em;
  font-size:2.4em;
}
.logo:hover .logo_text, .cartlink:hover a, .cartlink:hover span  {
  color: #ccc !important;
  transition:0.5s all;
  -webkit-transition:0.5s all;
  -o-transition:0.5s all;
}
.mobile_only {
  display:none;
}
.main_nav li a {
  font-family: "bebas_neuebook";
}
.slide-menu {
  height: 56px;
  position: fixed;
  cursor:pointer;
  right: 0;
  top: 0;
  width: 70px;
  background: url({{'hamburger.png' | asset_url }}) no-repeat scroll;
  background-size:35px 25px;
  background-position:center center;
  background-color:rgba(0, 0, 0, 0.9);
  z-index:500;
}
.slide-menu:hover {
  background: url({{'hamburger_black.png' | asset_url }}) no-repeat scroll;
  background-color:rgba(0, 0, 0, 0.5);
  background-size:35px 25px;
  background-position:center center;
}
.navigation {
  background: #E6E7E8;
  right: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index:600;
  transition: all 1s ease 0s;
  -o-transition: all 1s ease 0s;
  -webkit-transition: all 1s ease 0s;
}
#pageheader {
  position:fixed;
}

.navigation ul {
  margin: 0;
  text-align: center;
}
.navigation ul li {
  display:inline-block;
}
.navigation ul li a {
  color: #000;
  font-family: 'bebas_neuebook';
  font-size: 15pt;
  letter-spacing:0.1em;
  padding: 16px 12px;
  display:block;
  text-transform: uppercase;
}
.navigation li.active a{
  color:#fff;
}
.navigation li.active {
  /*background: #1a2d58 none repeat scroll 0 0;
  border-left: 2px solid #fff;
  border-right: 2px solid #fff;*/

}
.push-menu .navigation ul li:hover a {
  color:#999;
}
.cartlink {
  position:fixed;
  right:21px;
  top:0;
  z-index:700;
}
.homepage {
  background:#fff;
}
.cart-icon {
  display:block;
  width:70px;
  height:56px;
  position:relative;
  line-height:56px;
  font-size:15pt;
  color:#000 !important;

}

.cart-icon > span {
  color: #000;
  height: 23px;
  line-height: 22px;
  padding-left: 6px;
  width: 13px;
}
.collection_dots .dot {
  border-radius:0;
  height:25px;
  width:25px;
  border:none !important;
  background-size:25px !important;
}
.collection_dots .dot.dn-arw {
  background:url({{'db.png'| asset_url}}) no-repeat;

}
.collection_dots .dot.up-arw {
  background:url({{'ub.png'| asset_url}}) no-repeat; 
}
.collection_dots .dot.active {
  background:url({{'df.png'| asset_url}}) no-repeat; 
}
.collection_dots span.dot.active:last-child  {
  background:url({{'uf.png'| asset_url}}) no-repeat; 
}
.collection_preview.four .headline {
  bottom:10%;
}
